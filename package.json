  "main": "./dist/main/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --mode=development",
    "dev:build:renderer": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config ./webpack.config.renderer.dev.ts",
    "dev:build:main": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config ./webpack.config.main.dev.ts",
    "dev:clean": "rimraf ./dist",
    "dev:start:renderer": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.renderer.dev.ts",
    "dev:start:main": "cross-var wait-on $npm_package_main && cross-env NODE_ENV=development electron . --inspect=9229 --remote-debugging-port=9223",
    "dev:start:hmr": "run-p dev:build:main dev:start:renderer",
    "dev": "cross-env HMR=1 run-s dev:clean dev:start:hmr"
  },
  "keywords": [
    "typescript",
    "Electron",
    "webpack"
  ],
